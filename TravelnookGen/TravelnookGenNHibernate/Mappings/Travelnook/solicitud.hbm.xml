<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="TravelnookGenNHibernate"
		                   namespace="TravelnookGenNHibernate.EN.Travelnook">
		    
		    
			
				<class name="SolicitudEN" table="Solicitud"> 
				  	
						<id name="Id" column="id" type="integer"> 
							<generator class="identity"/>
						</id>
			
					
					<property name="Estado" > 
						<column name="estado"  not-null="true"/>
					</property>
					<property name="Fecha" type="date"> 
						<column name="idUsuario"  not-null="true"/>
					</property>
			
			
		  			
						<many-to-one name="Solicitante" class="TravelnookGenNHibernate.EN.Travelnook.UsuarioEN" >	
								<column name="FK_nomUsu_idUsuario"/> 
						</many-to-one>
											 					
			
			
			
			
		  			
						<many-to-one name="Solicitado" class="TravelnookGenNHibernate.EN.Travelnook.UsuarioEN" >	
								<column name="FK_nomUsu_idUsuario_0"/> 
						</many-to-one>
											 					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="SolicitudENReadAllHQL">from SolicitudEN</query>
		  <query name="SolicitudENdevuelveSolicitudesHQL"><![CDATA[FROM SolicitudEN s where s.Solicitante.Email like '%'+:p_solicitante+'%' AND s.Solicitado.Email like '%'+:p_solicitado+'%']]></query>
		  <query name="SolicitudENdevuelveSolicitudesRecibidasHQL"><![CDATA[FROM SolicitudEN s where s.Solicitado.Email like '%'+:p_email+'%']]></query>
		  <query name="SolicitudENdevuelveSolicitudesEnviadasHQL"><![CDATA[FROM SolicitudEN s where s.Solicitante.Email like '%'+:p_email+'%']]></query>
		</hibernate-mapping>
	